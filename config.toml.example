[api]
url = "https://api.example.com"
college_id = 1
campus_id = 1
group_id = 1

[app]
refresh_interval = 300
cache_enabled = true
cache_ttl = 3600
current_theme = "dark"

[keymap]
prev_day = "Left"
cur_day = "Up"
next_day = "Right"

[keymap.selector]
Char = "o"

[keymap.settings]
Char = "s"

[keymap.exit]
Char = "q"
